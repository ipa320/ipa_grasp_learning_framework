#!/usr/bin/env python

import rospy
from ipa_grasppose_dnn import PredictGraspQuality


predict_params = {"model_filepath": "/home/rmb-mr/.ros/models/session_13/model_028.h5",
                  "depth_image_dim": [64, 64, 1],  # the same as in training
                  "grasp_data_dim": [8, 8, 4]}  # the same as in training


if __name__ == "__main__":
    rospy.init_node("ipa_predict_graspquality")
    predictor = PredictGraspQuality(predict_params)
    rospy.spin()
